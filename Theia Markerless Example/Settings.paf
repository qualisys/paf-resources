Project ID: Open PAF Theia Markerless Example
Package Information:
  Name: PAF Theia Markerless Example
  Version: 1.0.0
  Required QTM version: 2.16.3600 
Root type: Subject
Default fields: [Creation date, Creation time]

Date format: Iso
Date separator: Dash
Filename filter: \.(pdf|r3w|cmo|docx)$

Types:
  Subject:
    Participant:
      Fields: [ID, Name, Sex, DOB, Height, Weight]
      Children: [Session]
      Directory pattern: $Name$_$ID$

  Session:
    Markerless Session:
      Fields: [Session ID, Test condition, Operator]
      Measurements: [Dynamic]
      Analyses: [ Step 1 - Theia Processing, Step 2 - Visual3D Analysis]
      Display order: [Session ID, Test condition, Operator, Comments]
      Directory pattern: $Creation date$_Markerless

Measurements:
  Fields:
    Measurement length: 10
    Minimum count: 1
    Maximum count: 1
    Count: 1
  Dynamic:
    Maximum count: 5
    Count: 3
    Measurement type: Dynamic

Analyses:
  Step 1 - Theia Processing:
    Type: Compound
    Components: [Internal Run Theia, Prepare Session Folder, Change Batch Commands and Preferences File, Wait Until Theia Idle]
  Internal Run Theia:
    Type: External program
    Program display name: Theia
    Do not wait for Application: Yes
    Arguments: [-path, $TemplateDirectory$Scripts\src\Theia\theia_batch_commands.txt, -force-single-instance]
  Prepare Session Folder:
    Type: Instantiate template
    Export session: Yes
    Template: Templates\Scripts\src\Theia\run_theia_tools.php
    Export measurements: [c3d,xml settings]
  Change Batch Commands and Preferences File:
    Type: Instantiate template
    Template: Templates\Scripts\src\Theia\run_theia_tools.php
  Wait Until Theia Idle:
    Type: Instantiate template
    Template: Templates\Scripts\src\Theia\run_theia_tools.php
  Step 2 - Visual3D Analysis:
    Type: External program
    Program display name: Visual 3d
    Export session: Yes
    Export measurements: c3d
    Arguments: [/s, $WorkingDirectory$create_workspace.v3s]
    Template files: ['*.v3s']
    Do not wait for Application: Yes

Fields:
  ID:
    Type: String
    Inherit: Connect
  Name:
    Type: String
    Inherit: Connect
    Force: Yes
  DOB:
    Type: Date
    Inherit: Connect
  Creation date:
    Type: Date
    Readonly: Yes
  Creation time:
    Type: TimeOfDay
    Readonly: Yes
  Session ID:
    Type: String
    Inherit: Connect
  Operator:
    Type: String
    Inherit: Copy
  Height:
    Type: Float
    Quantity: Length
    Unit: centimeters
    Decimals: 0
    Inherit: Connect
    Max: 250
    Min: 50
  Weight:
    Type: Float
    Quantity: Mass
    Unit: kilograms
    Decimals: 1
    Inherit: Copy
    Max: 250
    Min: 0
  Sex:
    Type: Enum
    Values: [Unspecified, Female, Male, Other]
    Inherit: Connect
    Default: Unspecified
  Measurement type:
    Type: String
  Test condition:
    Type: String

Columns:
  Default column definitions:
    ID:
      Width: 200
      Fields:
        Default: Filename
        Participant: ID
    Name:
      Width: 150
      Fields: 
        Participant: Name
    Created:
      Width: 65
      Field: Creation date
    Comments:
      Field: Comments
      Width: 100